// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Character {
  id           Int      @id @default(autoincrement())
  name         String
  life         Int      @default(50)
  force        Int
  createdAt    DateTime @default(now())
  agility      Int
  velocity     Int
  charisma     Int
  inteligence  Int
  constitution Int
  inventory    String   @default("Invent√°rio vazio")
  role         String
}
